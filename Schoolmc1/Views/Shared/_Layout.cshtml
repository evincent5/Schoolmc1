<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
</head>
<body style="padding-top:70px">
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed"
                        data-target="#Links" data-toggle="collapse">

                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("School", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                <a href="#" class="navbar-brand">School</a>
            </div>
            <div class="collapse navbar-collapse" id="Links">
                <ul class="nav navbar-nav ">
                    <li class="text-warning" >
                        @Html.ActionLink("Request", "Index", "Home")
                    </li>
                    <li class="text-warning">
                        @Html.ActionLink("Course", "index", "Courses")
                    </li>
                </ul>
            </div>
        </div>
    </nav>


    <div class="container">
        @RenderBody()
    </div>
    @RenderSection("footer", required: true)



    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
